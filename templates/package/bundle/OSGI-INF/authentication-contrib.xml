<component name="<%- name %>.authentication.contrib">
  <extension
    target="org.nuxeo.ecm.platform.ui.web.auth.service.PluggableAuthenticationService"
    point="startURL">
    <startURLPattern>
      <patterns>
        <pattern><%= name %>/</pattern>
      </patterns>
    </startURLPattern>
  </extension>
</component>
